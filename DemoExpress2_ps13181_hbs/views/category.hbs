        function goToAddNew() {
            window.location.href = '/products/add_category/'
        }
        function goToEditCategory() {
            window.location.href = '/products/edit_category/' + _id
        }
        function deleteCategory(id) {
                $.ajax({
                    type: 'DELETE',
                    url: '/products/category/delete/' + id,
                    success: function (data) {
                        if (data.res == true) {
                            window.location.href = '/products/category'
                        }
                    }
                });
            }
